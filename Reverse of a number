import java.util.Scanner;
public class Main {
	static void reverse(int n){
		int num = 0;
		while(n>0){
			int lDigint= n%10;
			n = n/10;
			num = (num * 10) + lDigint;
		}
		System.out.print(num);
	}
	
	public static void main(String[] args) {
		// Write your code here
		Scanner input = new Scanner(System.in);
		int n = input.nextInt();
		reverse(n);

	}
}
