import java.util.* ;
import java.io.*; 

public class Solution {
    static int countDigit(long x) {
        // Write your code here.
        int count = 0;
        while(x>0){
            long lDigit = x%10;
            count++;
            x = x/10;

        }
    
        return count;
    }
    public static void main(String[]args){
        Scanner input = new Scanner(System.in);
        long x = input.nextLong();
        countDigit(x);
    }
}
